{

    "manifest_version": 2,
    "name": "Phanout",
    "version": "1.0",
    "description": "Augments Guardian pages with Ophan data",
    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.guardian.co.uk/*",
                        "*://*.theguardian.com/*"],
            "js": ["phanout.js"]
        }
    ]

}
